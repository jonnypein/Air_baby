<br>
<h1>Clothes I'm currently renting! </h1>
<br>
    <div class="container">
      <div class="cards">
      <% @bookings.each do |booking| %>
      <div class="card-trip">
      <div class="card-trip-infos">
        <h2><%= booking.garment.title %> </h2>
        <br>
        <img src="https://images.unsplash.com/photo-1563722205706-e8513b7554bc?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1500&q=80" width="200" height="100" />
        <p class="booking_cards">You need to return your item by the: <%= booking.end_date %></p>
        <br>
        <%= link_to 'Delete Booking', booking_path(booking), method: :delete, data: { confirm: 'Are you sure?' }  %>
</div>

<% end %>

