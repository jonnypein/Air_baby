<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>

<h1 id='title_id'>Your rented items</h1>
    <div id="container_bookings_product">
      <% @bookings.each do |booking| %>
      <div id="cards_booking_product">
        <div class="card-product">
          <img src="https://images.unsplash.com/photo-1563722205706-e8513b7554bc?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1500&q=80" width="200" height="100" />
          <div class="card-product-infos">
            <h2 id='title_booking_big'><%= booking.garment.title %></h2>
            <p id='p_booking_big'>You need to return your item by the: <%= booking.end_date %></p>

            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
            <a class="btn btn-gradient" <%= link_to 'Delete Booking', booking_path(booking), method: :delete, data: { confirm: 'Are you sure?' } %></a>

           </div>
        </div>
      </div>
      <% end %>
    </div>

